#include <stdio.h>

int main() {
    int arr[3][3] = { {1, 2, 3}, {4, 5, 6}, {7, 8, 9} };

    printf("Addresses and Elements in Reverse Order:\n");

  
    for (int i = 2; i >= 0; i--) {
        for (int j = 2; j >= 0; j--) {
            printf("Address: %p, Element: %d\n", (void*)&arr[i][j], arr[i][j]);
        }
    }

    return 0;
}
